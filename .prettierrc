{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"bracketSameLine": true,
	"semi": false,
	"organizeImportsSkipDestructiveCodeActions": true,
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	],
	"pluginSearchDirs": false
}
